pipeline{
    agent any
    environment{
        GIT_REPO = "https://github.com/tejashree-dev/website01.git"
        GIT_BRANCH = "master"
        DOCKER_IMAGE = "warkad/newprojec"
        DOCKER_CREDENTIALS = ""

    }
    stages{
        stage ('git-repo stage') {
            steps{
             git credentialsId: '', url: "${GIT_REPO}" , branch: "${GIT_BRANCH}"
            }
            }
        }
        stage ('create docker image'){
        steps {
            sh "docker build -t $ (DOCKER_IMAGE) ."
        }
     }
     stage ('push docker image'){
        steps {
            sh "docker push $(DOCKER_IMAGE)"
        }
     }
     stage ('ec2 instance') {
        steps{

        }
     }
     post {
        always{
            echo "pipeline is completed"
        }
     }
    }

}
